{
  "name": "@api3/operations-utilities",
  "version": "0.0.1",
  "description": "Utilities common to API3 operations",
  "main": "./dist/index.js",
  "types": "./dist/index.js",
  "repository": "git@github.com:api3dao/telemetry.git",
  "author": "Aaron Scheiner <blue@aquarat.co.za>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "engine": "^16.17.0",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./dist *.tgz",
    "compile": "tsc -p tsconfig.build.json",
    "main": "ts-node src/main.ts",
    "pack": "yarn pack",
    "prettier:check": "prettier --check \"./**/*.{js,sol,ts,md,yml,json}\"",
    "prettier:fix": "prettier --write \"./**/*.{js,sol,ts,md,yml,json}\" --loglevel silent",
    "prettier:write": "yarn prettier:fix",
    "lint": "yarn run lint:eslint && yarn prettier:check",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint:eslint:fix": "eslint . --ext .js,.ts --fix",
    "test": "jest --selectProjects unit",
    "test:e2e": "SILENCE_LOGGER=true jest --selectProjects e2e --runInBand",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.13",
    "@types/pg": "^8.6.5",
    "@types/pg-format": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "8.22.0",
    "eslint-plugin-functional": "^4.2.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.1",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "@api3/operations": "^0.0.1-785b20f7ad527ae63c0f47f2a876da6c15c9a437",
    "@ethersproject/experimental": "^5.7.0",
    "axios": "^0.27.2",
    "ethers": "^5.7.0",
    "pg": "^8.8.0",
    "pg-format": "^1.0.4",
    "source-map-support": "^0.5.21",
    "zod": "^3.18.0"
  }
}
